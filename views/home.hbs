{{#if link}}
<h1>
    <a href={{link}}>{{msg}}</a>
</h1>
{{else}}
<h5>
    <a href="/followers">Next</a>
</h5>
{{#each list}}
<div>
    <img src={{this.profile_image_url}} alt="">
    <h3>{{this.screen_name}}</h3>
    <p>Name: {{this.name}} Followers: {{this.followers_count}}</p>
</div>
<hr>
{{/each}}
{{/if}}

{{#if logged}}
<footer>
    <h5>
        <a href="/followers">Next</a>
    </h5>
    <h5>
        <a href="/logout">Logout</a>
    </h5>
</footer>
{{/if}}